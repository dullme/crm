
page{
	background: #19233C;
}
.body{
	padding: 0;
	width: 100%;
	height: 100%;
	color: white;
	background: #19233C;
	color:#E9EAEB;
}
.real-body{
	margin-top: 40rpx;
	margin-bottom: 200rpx;
	padding: 0rpx 50rpx 0 50rpx;
	background-size:100%;
}
.my-box-card{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	background-image: -webkit-linear-gradient(301deg, #00B9EA 30%, #0099F0 61%, #0084F4 100%);
	background-image: linear-gradient(149deg, #00B9EA 30%, #0099F0 61%, #0084F4 100%);
	border-radius: 10rpx;
	padding: 50rpx 40rpx;
}
.my-box-card > image{
	width: 130rpx;
	height: 130rpx;
	border-radius: 130rpx;
	margin-right: 25rpx;
}
.my-code{
	margin-top: 10rpx;
	font-size: 30rpx;
	color: #B1EAFF;
}
.has-message:before{
	content: "\25CF";
	color: #F65D6B;
	font-size: 30rpx;
	position: absolute;
	margin-left: -26rpx;
	margin-top: -10rpx;
}
.memu-list{
	border-radius: 10rpx;
	background-color: #2A3452;
	margin-top: 25rpx;
}
.memu-pic{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	padding: 35rpx 0;
	border-bottom: 1px solid #323D5E;
}
.memu-list navigator, .memu-list > ._div{
	padding: 0 40rpx;
}
.left-pic{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 30rpx;
}
.left-pic > image{
	width: 40rpx;
	height: 40rpx;
	margin-right: 20rpx;
}
.memu-pic > image{
	width: 25rpx;
	height: 25rpx;
}


