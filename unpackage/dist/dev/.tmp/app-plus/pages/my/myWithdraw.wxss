
page{
	background: #19233C;
}
.body{
	padding: 0;
	width: 100%;
	height: 100%;
	color: white;
	background: #19233C;
	color:#E9EAEB;
}
.real-body{
	margin-top: 60rpx;
	padding: 0rpx 50rpx 0 50rpx;
}
.box-list{
	font-size: 30rpx;
}
.tr-pic{
	margin-bottom: 20rpx;
	padding: 0 50rpx;
	border-radius: 10rpx;
	background-color: #2A3452;
}
.tr-pic-title{
	font-size: 30rpx;
	color: #6A77A0;
	padding: 30rpx 0;
	border-bottom: 1px solid #323D5E;
}
.tr-pic-body{
	color: #6A77A0;
	padding: 30rpx 0;
}
.tr-pic-body > ._div{
	margin-bottom: 20rpx;
}
.tr-pic-body > ._div:last-child{
	margin-bottom: 0;
}
.color-white{
	color:#FFFFFF !important;
}
.color-blue{
	color:#40BCFF !important;
}
.complaint, .confirm{
	padding: 10rpx 20rpx;
	border-radius: 10rpx;
	font-size: 20rpx;
	float: right;
	margin-left: 20rpx;
}
.complaint{
	color: #FFAB32;
	border: 1px solid #FFAB32;
}
.confirm{
	color: #40BCFF;
	border: 1px solid #40BCFF;
}
.tr-pic-body-left{
	display: inline-block;
	min-width: 160rpx;
}

